#!/usr/bin/env node
import { readFileSync, writeFileSync } from "fs";

function patchRoom(path, overrides) {
  const raw = readFileSync(path, "utf8");
  const data = JSON.parse(raw);

  if (overrides.id) data.id = overrides.id;
  if (overrides.tier) data.tier = overrides.tier;
  if (overrides.domain) data.domain = overrides.domain;

  if (!data.title) {
    data.title = {
      en: data.name ?? "Weight Loss & Fitness",
      vi: data.name_vi ?? "Giảm Cân & Thể Dục",
    };
  }

  if (!data.content) {
    data.content = {
      en:
        data.description ??
        "Advanced strategies for weight loss and fitness with tracking.",
      vi:
        data.description_vi ??
        "Chiến lược nâng cao để giảm cân và cải thiện thể lực với theo dõi dữ liệu.",
    };
  }

  writeFileSync(path, JSON.stringify(data, null, 2));
  console.log("✅ Patched", path, "-> id:", data.id);
}

